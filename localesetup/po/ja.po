#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "使用法: ローカル設定 [locale]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "使用例: localesetup en_US"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "エラー: アーギュメントが多すぎます。"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "ローカルを $locale　に設定する。"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "エラー: $newlocale は、正規のローカルではありません。"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "システム用言語設定"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "閉じる"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "現行のローカルは、$currentlocaleです。"

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "システム用の言語を選択してください。"

#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "ANWENDUNG: localesetup [Sprache]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "BEISPIEL: localesetup de_DE"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "FEHLER: Zu viele Argumente"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "Sie benötigen Root-Rechte, um dies auszuführen"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "Setze die Sprache auf $locale"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "FEHLER: $newlocale ist keine gültige Spracheinstellung"

#: localesetup:76
#, sh-format
msgid "System language configuration"
msgstr "Konfiguration der Sprach- und Ländereinstellung"

#: localesetup:78
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:79
#, sh-format
msgid "Exit"
msgstr "Beenden"

#: localesetup:81
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Momentan ist die Sprache auf $currentlocale gesetzt."

#: localesetup:82
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Bitte wählen Sie ihre bevorzugte Sprache:"

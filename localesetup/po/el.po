#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "ΧΡΗΣΗ: localesetup [locale]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "ΠΑΡΑΔΕΙΓΜΑ: localesetup el_GR"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ΣΦΑΛΜΑ: Πολλές παράμετροι"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "Ρύθμιση της γλώσσας συστήματος σε $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "ΣΦΑΛΜΑ: Το $newlocale δεν είναι έγκυρη γλώσσα συστήματος"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "Ρύθμιση γλώσσας συστήματος"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "Εντάξει"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "Έξοδος"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Η τρέχουσα γλώσσα συστήματος είναι $currentlocale."

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Παρακαλώ επιλέξτε τη γλώσσα συστήματος."

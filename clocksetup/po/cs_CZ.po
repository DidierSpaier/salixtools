#: clocksetup:7
#, sh-format
msgid "You need root privileges to run this"
msgstr ""

#: clocksetup:46
#, sh-format
msgid "USAGE: clocksetup [[-c ] [-d] [-t] [-z]]"
msgstr "POUŽITÍ: clocksetup [[-c ] [-d] [-t] [-z]]"

#: clocksetup:48
#, sh-format
msgid "OPTIONS:"
msgstr "MOŽNOSTI:"

#: clocksetup:50
#, sh-format
msgid "   -c,   only show the hardware clock configuration dialog"
msgstr ""

#: clocksetup:51
#, sh-format
msgid "   -d,   only show the date configuration dialog"
msgstr ""

#: clocksetup:52
#, sh-format
msgid "   -t,   only show the time configuration dialog"
msgstr ""

#: clocksetup:53
#, sh-format
msgid "   -z,   only show the timezone configuration dialog"
msgstr ""

#: clocksetup:54
#, sh-format
msgid "   -h,   this help message"
msgstr ""

#: clocksetup:93
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "CHYBA: Příliš mnoho argumentů"

#: clocksetup:103 clocksetup:167 clocksetup:188 clocksetup:209 clocksetup:259
#, sh-format
msgid "OK"
msgstr "OK"

#: clocksetup:104
#, sh-format
msgid "Exit"
msgstr "Opustit"

#: clocksetup:105
#, sh-format
msgid "System clock configuration"
msgstr "Nastavení systémových hodin"

#: clocksetup:107
#, sh-format
msgid "Currently the clock is set to:"
msgstr "Aktuální nastavení hodin:"

#: clocksetup:108
#, sh-format
msgid "Please choose an option:"
msgstr "Prosím, zvolte jednu z možností:"

#: clocksetup:110
#, sh-format
msgid "Set the time manually"
msgstr "Nastavení času ručně"

#: clocksetup:111
#, sh-format
msgid "Set the date manually"
msgstr "Nastavení datumu ručně"

#: clocksetup:112
#, sh-format
msgid "Set the time zone"
msgstr "Nastavení časového pásma"

#: clocksetup:113
#, sh-format
msgid "Configure the hardware clock"
msgstr "Nastavení hardwarových hodin"

#: clocksetup:114
#, sh-format
msgid "Synchronize with Network Time Protocol (NTP)"
msgstr "Synchronizace pomocí Network Time Protocolu (NTP)"

#: clocksetup:166
#, sh-format
msgid "Clock adjustment"
msgstr "Nastavení hodin"

#: clocksetup:168 clocksetup:189 clocksetup:210 clocksetup:260
#, sh-format
msgid "Cancel"
msgstr "Zrušit"

#: clocksetup:170
#, sh-format
msgid "Please set the time..."
msgstr "Prosím nastavte čas..."

#: clocksetup:187
#, sh-format
msgid "Date adjustment"
msgstr "Nastavení data"

#: clocksetup:191
#, sh-format
msgid "Please set the date..."
msgstr "Prosím nastavte datum..."

#: clocksetup:211
#, sh-format
msgid "Time zone configuration"
msgstr "Nastavení časového pásama"

#: clocksetup:213
#, sh-format
msgid "Please select your time zone..."
msgstr "Prosím vyberte Vaše časové pásmo..."

#: clocksetup:233
#, sh-format
msgid "Warning"
msgstr "Varování"

#: clocksetup:235
#, sh-format
msgid "You are already using the ntpd service and clock should be correct."
msgstr "Již používáte ntpd službu a hodiny by měly být správně."

#: clocksetup:239
#, sh-format
msgid "Error"
msgstr "Chyba"

#: clocksetup:241
#, sh-format
msgid "Something went wrong. Time was not adjusted."
msgstr "Došlo k chybě. Čas nebyl upraven."

#: clocksetup:261
#, sh-format
msgid "Hardware clock set to UTC?"
msgstr "Nastavit hardwarové hodiny na UTC?"

#: clocksetup:263
#, sh-format
msgid ""
"Is the hardware clock set to Coordinated Universal Time (UTC/GMT)? If it is, "
"select YES here.  If the hardware clock is set to the current local time "
"(this is how most PCs are set up), then say NO here.  If you are not sure "
"what this is, you should answer NO here."
msgstr ""

#: clocksetup:264
#, sh-format
msgid "No"
msgstr "Ne"

#: clocksetup:264
#, sh-format
msgid "Hardware clock is set to local time"
msgstr "Nastavit hardwarové hodiny na lokální čas"

#: clocksetup:265
#, sh-format
msgid "Yes"
msgstr "Ano"

#: clocksetup:265
#, sh-format
msgid "Hardware clock is set to UTC"
msgstr "Hardwarové hodiny jsou nastaveny na UTC"

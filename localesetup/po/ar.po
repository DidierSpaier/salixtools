#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "إسنعمال: localesetup [locale]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "مثال: localsetup ar_MA"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "خطا: العديد من المعطيات"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "إعداد اللغة إلى $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "خطأ: $newlocale ليست لغة صحيحة"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "إعدادات لغة النظام"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "نعم"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "خروج"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "اللغة الحالية هي $currentlocale."

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "المرجوا إختيار لغة النظام المفضلة:"

#: localesetup:22
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "USO: localesetup [idioma]"

#: localesetup:23
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "Exemplo: localesetup pt_PT"

#: localesetup:28
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "Erro: demasiados argumentos"

#: localesetup:34
#, sh-format
msgid "You need root privileges to run this"
msgstr "Você precisa de privilégios root para executar isto"

#: localesetup:42
#, sh-format
msgid "Setting locale to $locale"
msgstr "A definir idioma para $locale"

#: localesetup:44
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "Erro: $newlocale não é um idioma válido"

#: localesetup:76
#, sh-format
msgid "System language configuration"
msgstr "Sistema de configuração de idioma"

#: localesetup:78
#, sh-format
msgid "OK"
msgstr "Confirmar"

#: localesetup:79
#, sh-format
msgid "Exit"
msgstr "Sair"

#: localesetup:81
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "O idioma atual é $currentlocale."

#: localesetup:82
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Por favor escolha o idioma preferencial para o sistema:"

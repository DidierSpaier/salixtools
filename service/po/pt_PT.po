#: service:47
#, sh-format
msgid ""
"${BOLDCYAN}Starting${COLOR_RESET} the "
"${BOLDWHITE}${servicename}${COLOR_RESET} service"
msgstr "${NEGRITOCIANO}Começando${REDEFINIR_CORES}o\n${NEGRITOBRANCO}${nomeserviço}${REDEFINIR_CORES}serviço"

#: service:51 service:63 service:81
#, sh-format
msgid "No ${BOLDWHITE}${servicename}${COLOR_RESET} service"
msgstr "Não ${BRANCONEGRITO}${nomedeserviço}${REINICIAR_COR} serviço"

#: service:57
#, sh-format
msgid ""
"${BOLDYELLOW}Stopping${COLOR_RESET} the "
"${BOLDWHITE}${servicename}${COLOR_RESET} service"
msgstr "${AMARELONEGRITO}Parando${REINICIAR_COR} o\n${BRANCONEGRITO}${nomedeserviço}${REINICIAR_COR} serviço"

#: service:70
#, sh-format
msgid ""
"${BOLDCYAN}Restarting${COLOR_RESET} the "
"${BOLDWHITE}${servicename}${COLOR_RESET} service"
msgstr "${CIANONEGRITO}Reiniciando${REINICIAR_COR} o\n${BRANCONEGRITO}${nomedeserviço}${REINICIAR_COR} serviço"

#: service:90
#, sh-format
msgid "The $service service"
msgstr "O $serviço serviço"

#: service:113
#, sh-format
msgid "usage : service start|stop|restart|list [service_name]"
msgstr "uso : serviço de início | parar | reiniciar | listar [nome_serviço]"

#: ../clocksetup:49
#, sh-format
msgid "USAGE: clocksetup [[-c ] [-d] [-t] [-z]]"
msgstr "UŻYCIE: clocksetup [[-c] [-d] [-t] [-z]]"

#: ../clocksetup:51
#, sh-format
msgid "OPTIONS:"
msgstr "OPCJE:"

#: ../clocksetup:53
#, sh-format
msgid "   -c,   only show the hardware clock configuration dialog"
msgstr "-c, wyświetla jedynie okno z ustawieniami zegara sprzętowego"

#: ../clocksetup:54
#, sh-format
msgid "   -d,   only show the date configuration dialog"
msgstr "-d, wyświetla jedynie okno z ustawieniami daty"

#: ../clocksetup:55
#, sh-format
msgid "   -t,   only show the time configuration dialog"
msgstr "-t, wyświetla jedynie okno z ustawieniami czasu"

#: ../clocksetup:56
#, sh-format
msgid "   -z,   only show the timezone configuration dialog"
msgstr "-z, wyświetla jedynie okno z ustawieniami strefy czasowej"

#: ../clocksetup:57
#, sh-format
msgid "   -h,   this help message"
msgstr "-h, obecna wiadomość pomocy"

#: ../clocksetup:96
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "BŁĄD: Zbyt wiele argumentów"

#: ../clocksetup:106 ../clocksetup:170 ../clocksetup:191 ../clocksetup:212
#: ../clocksetup:262
#, sh-format
msgid "OK"
msgstr "OK"

#: ../clocksetup:107
#, sh-format
msgid "Exit"
msgstr "Wyjdź"

#: ../clocksetup:108
#, sh-format
msgid "System clock configuration"
msgstr "Ustawienia zegara systemowego"

#: ../clocksetup:110
#, sh-format
msgid "Currently the clock is set to:"
msgstr "Obecnie zegar ustawiony jest na:"

#: ../clocksetup:111
#, sh-format
msgid "Please choose an option:"
msgstr "Wybierz opcję:"

#: ../clocksetup:113
#, sh-format
msgid "Set the time manually"
msgstr "Ustaw czas ręcznie"

#: ../clocksetup:114
#, sh-format
msgid "Set the date manually"
msgstr "Ustaw datę ręcznie"

#: ../clocksetup:115
#, sh-format
msgid "Set the time zone"
msgstr "Ustaw strefę czasową"

#: ../clocksetup:116
#, sh-format
msgid "Configure the hardware clock"
msgstr "Skonfiguruj zegar sprzętowy"

#: ../clocksetup:117
#, sh-format
msgid "Synchronize with Network Time Protocol (NTP)"
msgstr "Zsynchronizuj z Network Time Protocol (NTP)"

#: ../clocksetup:169
#, sh-format
msgid "Clock adjustment"
msgstr "Ustawienia czasu"

#: ../clocksetup:171 ../clocksetup:192 ../clocksetup:213 ../clocksetup:263
#, sh-format
msgid "Cancel"
msgstr "Anuluj"

#: ../clocksetup:173
#, sh-format
msgid "Please set the time..."
msgstr "Ustaw czas..."

#: ../clocksetup:190
#, sh-format
msgid "Date adjustment"
msgstr "Ustawienia daty"

#: ../clocksetup:194
#, sh-format
msgid "Please set the date..."
msgstr "Ustaw datę..."

#: ../clocksetup:214
#, sh-format
msgid "Time zone configuration"
msgstr "Ustawienia strefy czasowej"

#: ../clocksetup:216
#, sh-format
msgid "Please select your time zone..."
msgstr "Wybierz swoją strefę czasową..."

#: ../clocksetup:236
#, sh-format
msgid "Warning"
msgstr "Ostrzeżenie"

#: ../clocksetup:238
#, sh-format
msgid "You are already using the ntpd service and clock should be correct."
msgstr "Obecnie korzystasz już z usługi ntpd i zegar powinien być ustawiony poprawnie."

#: ../clocksetup:242
#, sh-format
msgid "Error"
msgstr "Błąd"

#: ../clocksetup:244
#, sh-format
msgid "Something went wrong. Time was not adjusted."
msgstr "Coś poszło nie tak. Czas nie został ustawiony."

#: ../clocksetup:264
#, sh-format
msgid "Hardware clock configuration"
msgstr "Ustawienia zegara systemowego"

#: ../clocksetup:266
#, sh-format
msgid "Is the hardware clock set to Coordinated Universal Time (UTC/GMT)? If it is, select YES here.  If the hardware clock is set to the current local time (this is how most PCs are set up), then say NO here.  If you are not sure what this is, you should answer NO here."
msgstr ""

#: ../clocksetup:267
#, sh-format
msgid "No"
msgstr "Nie"

#: ../clocksetup:267
#, sh-format
msgid "Hardware clock is set to local time"
msgstr ""

#: ../clocksetup:268
#, sh-format
msgid "Yes"
msgstr "Tak"

#: ../clocksetup:268
#, sh-format
msgid "Hardware clock is set to UTC"
msgstr ""

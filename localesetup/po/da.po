#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "BRUG: localesetup [lokalitet]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "EKSEMPEL: localesetup da"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "FEJL: For mange argumenter"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "Indstiller lokalitet til $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "FEJL: $newlocale er ikke en gyldig lokalitet"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "Konfiguration af systemsprog"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "O.k."

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "Afslut"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Den nuværende lokalitet er $currentlocale."

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Vælg venligst dit foretrukne systemsprog:"

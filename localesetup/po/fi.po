#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "KÄYTTÖ: localesetup [kieliasetus]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "ESIMERKKI: localesetup fi_FI"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "VIRHE: Liian monta argumenttia"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "Asetetaan kieliasetukseksi $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "VIRHE: $newlocale ei ole pätevä kieliasetus."

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "Järjestelmän kieliasetukset"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "Poistu"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Nykyinen kieliasetus on $currentlocale."

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Ole hyvä ja valitse järjestelmäsi oletuskieli:"

#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "Használat: localesetup [lokalizáció]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "Példa: localesetup en_US"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "HIBA: Túl sok paraméter"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "Nyelvi környezet beálítva erre $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "HIBA: $newlocale nem valós nyelvi környezet változó"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "Rendszer nyelvi környezet konfiguráció"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "Kilépés"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "A jelenlegi nyelvi környezet: $currentlocale"

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Kérem válassza ki az Ön által előnyben részesített nyelvet:"

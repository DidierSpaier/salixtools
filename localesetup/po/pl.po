#: localesetup:34
#, sh-format
msgid "USAGE: localesetup [locale]"
msgstr "ZASTOSOWANIE: localesetup [lokalizacja]"

#: localesetup:35
#, sh-format
msgid "EXAMPLE: localesetup en_US"
msgstr "PRZYKŁAD: localesetup en_US"

#: localesetup:40
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "BŁĄD: Zbyt wiele argumentów"

#: localesetup:48
#, sh-format
msgid "Setting locale to $locale"
msgstr "Ustawienie lokalizacji na $locale"

#: localesetup:50
#, sh-format
msgid "ERROR: $newlocale is not a valid locale"
msgstr "BŁĄD: $newlocale nie jest prawidłową lokalizacją"

#: localesetup:75
#, sh-format
msgid "System language configuration"
msgstr "Konfiguracja języka systemu"

#: localesetup:77
#, sh-format
msgid "OK"
msgstr "OK"

#: localesetup:78
#, sh-format
msgid "Exit"
msgstr "Wyjście"

#: localesetup:80
#, sh-format
msgid "The current locale is $currentlocale."
msgstr "Lokalizacją jest $currentlocale."

#: localesetup:81
#, sh-format
msgid "Please select your preferred system language:"
msgstr "Proszę wybrać preferowany język systemu:"
